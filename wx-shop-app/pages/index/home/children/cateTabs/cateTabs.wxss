.cate-tabs {
    width: 100%;

}

.cate-tabs-scroll-list {
    width: 100%;
    white-space: nowrap;
}

.cate-tabs-content {
}

.cate-tabs-item {
    width: 150rpx;
    display: inline-block;
    text-align: center;
    color: #666;
    height: 100%;
    padding: 30rpx 0;
    font-size: 30rpx;
}

.cate-tabs-item.item-selected {
    color: #111;
    font-weight: bold;
    font-size: 32rpx;
}

.cate-tabs-item-text {
    display: inline-block;
}
